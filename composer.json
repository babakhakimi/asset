{
    "name": "lorito/asset",
    "description": "Asset for Lorito Platform",
    "license": "MIT",
    "authors": [
        {
            "name": "babakhakimimehr",
            "email": "babakhakimimehr@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Lorito\\Asset\\" : "src/"
        }
    },
    "require": {
        "php": "^7.3 || ^8.0"
    },
    "require-dev": {
        "illuminate/filesystem": "^8.0"
    },
    "suggest": {
        "illuminate/filesystem": "Allow using lorito/asset component outside of Laravel app.",
        "lorito/html": "Allow using lorito/asset with HTML component."
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
